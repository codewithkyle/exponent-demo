{
  "require": {
    "aelvan/imager": "v2.3.0",
    "anubarak/craft-relabel": "1.3.4",
    "chasegiunta/scss": "1.0.0",
    "craftcms/cms": "3.3.17",
    "craftcms/redactor": "2.4.0",
    "ether/logs": "3.0.3",
    "ether/seo": "3.6.2",
    "ether/splash": "3.0.5",
    "nystudio107/craft-eagerbeaver": "1.0.4",
    "sebastianlenz/linkfield": "1.0.19",
    "vaersaagod/matrixmate": "1.2.2",
    "verbb/smith": "1.0.0",
    "verbb/super-table": "2.3.1",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\pwamodule\\": "modules/pwamodule/src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
